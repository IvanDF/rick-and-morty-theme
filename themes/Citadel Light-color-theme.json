        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#66bb6a"
      }
    },
    {
      "name": "String - Portal Fluid",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#f57f17"
      }
    },
    {
      "name": "Number - Dimension C-137",
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#d32f2f"
      }
    },
    {
      "name": "Boolean - True Morty",
      "scope": [
        "constant.language.boolean"
      ],
      "settings": {
